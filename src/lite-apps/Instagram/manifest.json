{
  "theme_color": "#253938",
  "manifest_version": 3,
  "name": "Instagram",
  "priority": 50,
  "start_url": "https://instagram.com/",
  "manifest_url": "https://lite-apps.chimbori.com/lite-apps/v3/Instagram.hermit",
  "secondary_color": "#213332",
  "icon": "favicon.png",
  "settings": {
    "block_third_party_cookies": false,
